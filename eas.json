{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "adhoc": {
      "distribution": "adhoc",
      "ios": {
        "resourceClass": "m-medium",
        "simulator": false
      },
      "env": {
        "GOOGLE_PLIST": "@env:GOOGLE_PLIST"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}